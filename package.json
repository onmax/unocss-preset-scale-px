{
  "name": "unocss-preset-scale-px",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@10.4.1",
  "description": "A UnoCSS preset that maps utility numbers directly to pixel values (e.g., p-4 â†’ 4px)",
  "author": "onmax",
  "license": "MIT",
  "homepage": "https://github.com/onmax/unocss-preset-scale-pxr#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/onmax/unocss-preset-scale-pxr.git"
  },
  "bugs": {
    "url": "https://github.com/onmax/unocss-preset-scale-pxr/issues"
  },
  "keywords": [
    "unocss",
    "unpreset",
    "unocss-preset",
    "unocss-preset-starter"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp --commit --push --tag && pnpm publish",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "test:update": "vitest --update"
  },
  "dependencies": {
    "@unocss/core": "^66.0.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.3.0",
    "@babel/types": "^7.26.9",
    "@types/node": "^22.13.4",
    "@unocss/eslint-plugin": "^66.0.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "bumpp": "^10.0.3",
    "eslint": "^9.20.1",
    "typescript": "5.7.3",
    "unbuild": "^3.3.1",
    "unocss": "^66.0.0",
    "vite": "^6.1.1",
    "vitest": "^3.0.6"
  }
}
